<p id="notice"><%= notice %></p>

<h1>Iterations</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Project</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @iterations.each do |iteration| %>
      <tr>
        <td><%= iteration.title %></td>
        <td><%= iteration.description %></td>
        <td><%= iteration.start_date %></td>
        <td><%= iteration.end_date %></td>
        <td><%= iteration.project_id %></td>
        <td><%= iteration.project %></td>
        <td><%= link_to 'Show', project_iteration_path(@project.id, iteration.id) %></td>
        <td><%= link_to 'Edit', edit_project_iteration_path(@project.id, iteration.id) %></td>
        <td><%= link_to 'Destroy', project_iteration_path(@project.id, iteration.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Iteration', new_project_iteration_path %>
